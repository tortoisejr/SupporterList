
@{
    ViewBag.Title = "edit";
}

<h2>edit</h2>

<form method="post">
    <input type="hidden" name="Id" class="form-control" value="@Model.Id"/>
    First Name: <input type="text" name="FName" class="form-control" value="@Model.FName" placeholder="First Name" />
    Last Name: <input type="text" name="LName" class="form-control" value="@Model.FName" placeholder="Last Name" />
    Age: <input type="text" name="Age" class="form-control" value="@Model.Age" />
    Nationality: <input type="text" name="Nation" class="form-control" value="@Model.Nation" />
    Supported Club: <input type="text" name="Club" class="form-control" value="@Model.Club" />
    <input type="submit" value="Update" class="btn btn-success" />
</form>